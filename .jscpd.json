{
  "threshold": 5.1,
  "reporters": ["console"],
  "ignore": [
    "**/target/**",
    "**/node_modules/**",
    "**/*.lock",
    "**/.sqlx/**",
    "**/migrations/**",
    "**/tests/**"
  ],
  "format": ["rust"],
  "absolute": true,
  "gitignore": true,
  "minLines": 10,
  "minTokens": 50,
  "skipLocal": false,
  "ignorePattern": ["#\\[cfg\\(test\\)\\][\\s\\S]*?\\n\\}"]
}
